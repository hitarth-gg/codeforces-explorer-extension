{
  "manifest_version": 3,
  "name": "codeforces-explorer",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": ["https://blank.org/*", "https://codeforces.com/profile/*", "https://codeforces.com/contest/*", "https://codeforces.com/problemset/*"],
      "js": ["content-script/src/main.tsx"],
      "media": []
    }
  ]
}
